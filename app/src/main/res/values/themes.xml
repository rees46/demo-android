<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Base.Theme.Demoandroid" parent="Theme.Material3.DayNight.NoActionBar">
    </style>

    <style name="Theme.Demoandroid" parent="Base.Theme.Demoandroid">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textColor">@color/default_textColor</item>
    </style>

    <style name="ThemeOverlay.Demoandroid.StartScreen" parent="">
        <item name="backgroundColor">@color/black</item>
        <item name="headerTextColor">@color/white</item>
    </style>

    <style name="SuggestTextRegular" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/inter</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="SuggestTextBold" parent="SuggestTextRegular">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="HomeBottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="backgroundTint">@color/black</item>
        <item name="itemRippleColor">@color/black</item>
        <item name="itemIconTint">@color/main_bottom_navigation_item_color</item>
        <item name="itemTextColor">@color/main_bottom_navigation_item_color</item>
        <item name="itemIconSize">@dimen/main_bottom_navigation_size</item>
    </style>

    <style name="Button" parent="">
        <item name="cornerRadius">@dimen/default_button_cornerRadius</item>
        <item name="android:textSize">@dimen/default_button_textSize</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="WhiteButton" parent="Button">
        <item name="strokeWidth">1dp</item>
        <item name="strokeColor">@color/black</item>
        <item name="backgroundTint">@color/white</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="BlackButton" parent="Button">
        <item name="backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>
